{
    "name": "prodom-documentation",
    "version": "1.0.0",
    "description": "Prodom is a declarative dom framework",
    "private": true,
    "sideEffects": [
        "*.css"
    ],
    "scripts": {
        "heroku-prebuild": "npm install --dev",
        "start": "npm run build && webpack serve  --port=$PORT",
        "run": "webpack serve --host 0.0.0.0",
        "frontend": "cd frontend && webpack serve",
        "build": "webpack --mode development",
        "lint": "eslint . --ext .ts",
        "lint-and-fix": "eslint . --ext .ts --fix",
        "prettier-format": "prettier --config .prettierrc './src/**/*.ts' --write",
        "watch": "webpack --watch",
        "test": "jest",
        "test-watch": "jest --watch",
        "heroku-postbuild": "npm run build"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.18.6",
        "@babel/preset-env": "^7.18.6",
        "@types/jest": "^26.0.23",
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2",
        "babel-loader": "^8.2.5",
        "clean-webpack-plugin": "^3.0.0",
        "crypto-browserify": "^3.12.0",
        "css-loader": "^5.0.2",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-prettier": "^3.3.1",
        "html-webpack-plugin": "^5.2.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "source-map-loader": "^3.0.0",
        "style-loader": "^2.0.0",
        "ts-jest": "^26.5.6",
        "ts-loader": "^8.0.17",
        "typescript": "^4.2.2",
        "webpack": "^5.24.1",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.9.2",
        "webpack-pwa-manifest": "^4.0.0"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "buffer-browserify": "^0.2.5",
        "dns": "^0.2.2",
        "express": "^4.18.1",
        "firebase": "^9.8.4",
        "install": "^0.13.0",
        "jssha": "^3.2.0",
        "keypair": "^1.0.4",
        "npm": "^8.13.1",
        "path-browserify": "^1.0.1",
        "prodom": "^0.0.4",
        "request": "^2.79.0",
        "stream-browserify": "^3.0.0"
    },
    "browser": {
        "crypto": false
    }
