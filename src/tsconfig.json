{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "es6",
		"target": "es5",
		"allowJs": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"paths": {
			"crypto": [
				"./node_modules/crypto-browserify"
			],
			"stream": [
				"./node_modules/stream-browserify"
			],
			"path": [
				"./node_modules/path-browserify"
			],
		}
	},
}